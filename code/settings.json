{
  "[json][jsonc][yaml]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[sql]": {
    "editor.defaultFormatter": "bradymholt.pgformatter"
  },
  "[dockercompose][dockerfile]": {
    "editor.defaultFormatter": "ms-azuretools.vscode-docker"
  },
  "color-highlight.markerType": "dot-after",
  "editor.autoClosingBrackets": "always",
  "editor.bracketPairColorization.enabled": true,
  "editor.codeActionsOnSave": {
    "source.organizeImports": "explicit"
  },
  "editor.cursorStyle": "line-thin",
  "editor.fontLigatures": true,
  "editor.fontSize": 14,
  "editor.formatOnSave": true,
  "editor.inlayHints.fontFamily": "Operator Mono, monospace",
  "editor.lineHeight": 21,
  "editor.minimap.enabled": false,
  "editor.renderWhitespace": "boundary",
  "editor.suggest.preview": true,
  "editor.tabSize": 2,
  "emmet.includeLanguages": {
    "postcss": "css"
  },
  "emmet.triggerExpansionOnTab": true,
  "errorLens.excludeBySource": ["cSpell"],
  "eslint.nodePath": "/usr/lib/node_modules",
  "eslint.validate": ["javascript", "javascriptreact"],
  "yaml.completion": true,
  "yaml.schemaStore.enable": true,
  "explorer.confirmDelete": false,
  "explorer.decorations.colors": false,
  "extensions.autoCheckUpdates": false,
  "extensions.autoUpdate": false,
  "extensions.ignoreRecommendations": true,
  "files.autoSave": "off",
  "git.allowForcePush": true,
  "git.autofetch": true,
  "git.confirmForcePush": false,
  "git.confirmSync": false,
  "go.lintOnSave": "package",
  "go.lintTool": "golangci-lint",
  "html.format.enable": false,
  "javascript.format.enable": false,
  "javascript.updateImportsOnFileMove.enabled": "always",
  "jest.autoRun": "off",
  "jest.coverageColors": {
    "covered": "rgba(64,128,128,0.5)",
    "partially-covered": "rgba(235, 198, 52, 0.25)",
    "uncovered": "rgba(128,64,64,0.25)"
  },
  "jest.testExplorer": {
    "enabled": true,
    "showInlineError": true
  },
  "jest.debugMode": true,
  "npm.runSilent": true,
  "pgFormatter.formatType": true,
  "prettier.jsxSingleQuote": true,
  "prettier.semi": false,
  "prettier.singleQuote": true,
  "redhat.telemetry.enabled": false,
  "repositories.treeAsDefault": true,
  "repositories.sortByName": true,
  "search.showLineNumbers": true,
  "search.useGlobalIgnoreFiles": true,
  "security.workspace.trust.enabled": false,
  "telemetry.telemetryLevel": "off",
  "terminal.integrated.fontFamily": "Operator Mono,monospace",
  "terminal.integrated.fontSize": 13,
  "terminal.integrated.fontWeightBold": "normal",
  "terminal.integrated.gpuAcceleration": "on",
  "typescript.format.enable": false,
  "typescript.surveys.enabled": false,
  "typescript.preferences.importModuleSpecifier": "project-relative",
  "update.mode": "none",
  "update.showReleaseNotes": false,
  "vscode_custom_css.imports": [
    "file:///home/mohit/.config/VSCodium/custom.css"
  ],
  "vscode_custom_css.statusbar": false,
  "window.closeWhenEmpty": true,
  "window.dialogStyle": "custom",
  "window.menuBarVisibility": "compact",
  "window.restoreFullscreen": true,
  "window.titleBarStyle": "custom",
  "workbench.editor.closeOnFileDelete": true,
  "workbench.editor.enablePreviewFromQuickOpen": false,
  "workbench.enableExperiments": false,
  "workbench.iconTheme": "file-icons",
  "workbench.layoutControl.enabled": false,
  "workbench.localHistory.enabled": false,
  "workbench.settings.editor": "json",
  "workbench.settings.enableNaturalLanguageSearch": false,
  "workbench.startupEditor": "none",
  "workbench.tips.enabled": false,
  "diffEditor.ignoreTrimWhitespace": false,
  "go.toolsManagement.autoUpdate": true,
  "workbench.colorTheme": "Tokyo Night Storm"
}
