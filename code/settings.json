{
  "Lua.telemetry.enable": false,
  "[dockerfile]": {
    "editor.defaultFormatter": "ms-azuretools.vscode-docker"
  },
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[sql]": {
    "editor.defaultFormatter": "bradymholt.pgformatter"
  },
  "[yaml]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "color-highlight.markerType": "dot-after",
  "diffEditor.renderSideBySide": false,
  "docker.showStartPage": false,
  "editor.codeActionsOnSave": {
    "source.organizeImports": true
  },
  "editor.cursorStyle": "line-thin",
  "editor.fontLigatures": true,
  "editor.fontSize": 14,
  "editor.formatOnSave": true,
  "editor.lineHeight": 21,
  "editor.minimap.enabled": false,
  "editor.renderWhitespace": "boundary",
  "editor.tabSize": 2,
  "emmet.includeLanguages": {
    "postcss": "css"
  },
  "emmet.triggerExpansionOnTab": true,
  "errorLens.excludeBySource": ["cSpell"],
  "eslint.nodePath": "/usr/lib/node_modules",
  "eslint.validate": ["javascript", "javascriptreact"],
  "explorer.confirmDelete": false,
  "explorer.decorations.colors": false,
  "extensions.autoCheckUpdates": false,
  "extensions.autoUpdate": "onlyEnabledExtensions",
  "extensions.ignoreRecommendations": true,
  "files.autoSave": "off",
  "files.exclude": {
    "**/.classpath": true,
    "**/.factorypath": true,
    "**/.project": true,
    "**/.settings": true
  },
  "files.insertFinalNewline": true,
  "git.allowForcePush": true,
  "git.autofetch": true,
  "git.confirmForcePush": false,
  "git.confirmSync": false,
  "go.formatTool": "gofumports",
  "go.lintOnSave": "package",
  "go.lintTool": "golangci-lint",
  "gopls": {
    "build.experimentalWorkspaceModule": true
  },
  "html.format.enable": false,
  "java.completion.enabled": true,
  "java.configuration.runtimes": [
    {
      "name": "JavaSE-1.8",
      "path": "/usr/lib/jvm/java-8-openjdk-amd64"
    },
    {
      "name": "JavaSE-16",
      "path": "/usr/lib/jvm/java-16-openjdk-amd64"
    }
  ],
  "java.home": "/usr/lib/jvm/java-16-openjdk-amd64",
  "javascript.format.enable": false,
  "javascript.updateImportsOnFileMove.enabled": "always",
  "jest.autoRun": "off",
  "jest.coverageColors": {
    "covered": "rgba(64,128,128,0.5)",
    "partially-covered": "rgba(235, 198, 52, 0.25)",
    "uncovered": "rgba(128,64,64,0.25)"
  },
  "npm.runSilent": true,
  "pgFormatter.keywordCase": "lowercase",
  "prettier.jsxSingleQuote": true,
  "prettier.semi": false,
  "prettier.singleQuote": true,
  "projectManager.git.baseFolders": [
    "/home/mohit/Projects",
    "/home/mohit/Work"
  ],
  "python.formatting.blackPath": "/usr/local/bin/black",
  "python.formatting.provider": "black",
  "python.languageServer": "Pylance",
  "python.linting.enabled": true,
  "python.linting.flake8Enabled": true,
  "python.linting.flake8Path": "/usr/local/bin/flake8",
  "python.linting.pylintEnabled": false,
  "python.pythonPath": "venv/bin/python",
  "redhat.telemetry.enabled": false,
  "rust-analyzer.cargo.allFeatures": true,
  "search.showLineNumbers": true,
  "search.useGlobalIgnoreFiles": true,
  "security.workspace.trust.enabled": false,
  "telemetry.enableCrashReporter": false,
  "telemetry.enableTelemetry": false,
  "terminal.integrated.fontFamily": "Dank Mono",
  "terminal.integrated.fontSize": 12,
  "terminal.integrated.lineHeight": 1.25,
  "typescript.format.enable": false,
  "typescript.surveys.enabled": false,
  "update.mode": "none",
  "update.showReleaseNotes": false,
  "vscode_custom_css.imports": [
    "file:///home/mohit/.config/VSCodium/custom.css"
  ],
  "vscode_custom_css.statusbar": false,
  "window.closeWhenEmpty": true,
  "window.dialogStyle": "custom",
  "window.menuBarVisibility": "compact",
  "window.restoreFullscreen": true,
  "window.titleBarStyle": "custom",
  "workbench.colorTheme": "One Dark Pro Darker",
  "workbench.editor.closeOnFileDelete": true,
  "workbench.editor.enablePreview": false,
  "workbench.editor.enablePreviewFromQuickOpen": false,
  "workbench.enableExperiments": false,
  "workbench.iconTheme": "file-icons",
  "workbench.settings.editor": "json",
  "workbench.settings.enableNaturalLanguageSearch": false,
  "workbench.startupEditor": "none",
  "workbench.statusBar.visible": true,
  "workbench.tips.enabled": false,
  "workbench.tree.renderIndentGuides": "none"
}
