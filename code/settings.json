{
  "[json][jsonc][yaml][scss][typescript][javascript][typescriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[sql]": {
    "editor.defaultFormatter": "bradymholt.pgformatter"
  },
  "color-highlight.markerType": "dot-after",
  "diffEditor.experimental.showMoves": true,
  "diffEditor.experimental.useTrueInlineView": true,
  "diffEditor.ignoreTrimWhitespace": false,
  "editor.autoClosingBrackets": "always",
  "editor.bracketPairColorization.enabled": true,
  "editor.codeActionsOnSave": {
    "source.organizeImports": "explicit"
  },
  "editor.cursorStyle": "line-thin",
  "editor.fontLigatures": true,
  "editor.fontSize": 14,
  "editor.formatOnSave": true,
  "editor.inlayHints.fontFamily": "Operator Mono, monospace",
  "editor.lineHeight": 1.5,
  "editor.minimap.enabled": false,
  "editor.renderWhitespace": "boundary",
  "editor.stickyScroll.enabled": true,
  "editor.suggest.preview": true,
  "editor.tabSize": 2,
  "emmet.includeLanguages": {
    "postcss": "css"
  },
  "emmet.triggerExpansionOnTab": true,
  "errorLens.excludeBySource": ["cSpell"],
  "eslint.validate": ["javascript", "javascriptreact"],
  "explorer.confirmDelete": false,
  "explorer.decorations.colors": false,
  "extensions.autoCheckUpdates": false,
  "extensions.autoUpdate": false,
  "extensions.ignoreRecommendations": true,
  "files.associations": {
    "*.sql": "postgres",
    "*.css": "tailwindcss"
  },
  "files.autoSave": "off",
  "go.gopath": "~/.go",
  "go.lintOnSave": "package",
  "go.lintTool": "golangci-lint",
  "go.toolsManagement.autoUpdate": true,
  "html.format.enable": false,
  "javascript.format.enable": false,
  "javascript.updateImportsOnFileMove.enabled": "always",
  "npm.runSilent": true,
  "pgFormatter.formatType": true,
  "pgFormatter.noSpaceFunction": true,
  "pgFormatter.spaces": 2,
  "plpgsqlLanguageServer.database": "postgres",
  "plpgsqlLanguageServer.host": "localhost",
  "plpgsqlLanguageServer.user": "postgres",
  "redhat.telemetry.enabled": false,
  "repositories.sortByName": true,
  "repositories.treeAsDefault": true,
  "search.showLineNumbers": true,
  "search.useGlobalIgnoreFiles": true,
  "security.workspace.trust.enabled": false,
  "tailwindCSS.experimental.classRegex": [
    ["cva\\(([^)]*)\\)", "[\"'`]([^\"'`]*).*?[\"'`]"],
    ["cx\\(([^)]*)\\)", "(?:'|\"|`)([^']*)(?:'|\"|`)"]
  ],
  "telemetry.telemetryLevel": "off",
  "terminal.integrated.fontFamily": "Operator Mono, Symbols Nerd Font",
  "terminal.integrated.fontSize": 13,
  "terminal.integrated.fontWeightBold": "normal",
  "terminal.integrated.gpuAcceleration": "on",
  "typescript.format.enable": false,
  "typescript.preferences.importModuleSpecifier": "project-relative",
  "typescript.surveys.enabled": false,
  "update.mode": "none",
  "update.showReleaseNotes": false,
  "vscode_custom_css.imports": [
    "file:///home/mohit/.config/VSCodium/custom.css"
  ],
  "vscode_custom_css.statusbar": false,
  "window.dialogStyle": "custom",
  "window.restoreFullscreen": true,
  "window.titleBarStyle": "custom",
  "workbench.colorTheme": "Tokyo Night Storm",
  "workbench.editor.closeOnFileDelete": true,
  "workbench.editor.enablePreviewFromQuickOpen": false,
  "workbench.enableExperiments": false,
  "workbench.iconTheme": "file-icons",
  "workbench.localHistory.enabled": false,
  "workbench.settings.editor": "json",
  "workbench.settings.enableNaturalLanguageSearch": false,
  "workbench.startupEditor": "none",
  "workbench.tips.enabled": false,
  "yaml.completion": true,
  "yaml.schemaStore.enable": true,
  "testing.automaticallyOpenTestResults": "neverOpen",
  "chat.commandCenter.enabled": false,
  "workbench.editor.editorActionsLocation": "titleBar",
  "git.blame.editorDecoration.enabled": true
}
